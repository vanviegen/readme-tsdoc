{
  "name": "readme-tsdoc",
  "version": "1.0.0",
  "description": "Automatically generate TypeScript documentation and insert it into your README.md file",
  "main": "readme-tsdoc.js",
  "type": "module",
  "bin": {
    "readme-tsdoc": "./readme-tsdoc.js"
  },
  "scripts": {
    "start": "node readme-tsdoc.js",
    "update-readme": "node readme-tsdoc.js",
    "prepublishOnly": "npm run update-readme",

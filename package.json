  },
  "keywords": [
    "typescript",
    "documentation",
    "readme",
    "tsdoc",
    "markdown",
    "docs"
  ],
  "author": "Frank van Viegen",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "typescript": "^5.0.0"
  },
  "files": [
    "readme-tsdoc.js",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanviegen/readme-tsdoc.git"
  },
  "bugs": {
    "url": "https://github.com/vanviegen/readme-tsdoc/issues"
